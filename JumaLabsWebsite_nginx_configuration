server {
	listen   8081; ## listen for ipv4; this line is default and implied

	root /home/justin/Documents/JumaLabs/www;
	index /pages/index.html;

	server_name www.jumalabs.com;

	location / {
		root /home/justin/Documents/JumaLabs/www;
		try_files $uri index.html @pages;
	}
	location @pages{
		rewrite	^/(.+)$ /pages/$1.html last;
	}
}
