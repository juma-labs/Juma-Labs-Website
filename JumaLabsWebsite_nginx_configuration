server {
	listen   80; ## listen for ipv4; this line is default and implied

	root /var/www/JumaLabsWebsite;
	index /pages/index.html;

	server_name www.jumalabs.com;

	location / {
		root /var/www/JumaLabsWebsite;
		try_files $uri index.html @pages;
	}
	location @pages{
		rewrite	^/(.+)$ /pages/$1.html last;
	}
}
